{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "func-names": "off",
    "no-undef": "off",
    "prefer-arrow-callback": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 100,
        "trailingComma": "es5"
      }
    ]
  },
  "env": {
    "node": true,
    "jest": true
  }
}